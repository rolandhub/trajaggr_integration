	pmid = {19545430},
	keywords = {density of park acreage, density of physical activity sites, geographically weighted (local) regression, gis, kernel density estimation, longitudinal case studies, measuring access, methods, network analysis, ordinary least squares (global) regression, park, physical activity, qualitative and quantitative},
	pages = {34},
	file = {Full Text PDF:/home/harry/.mozilla/firefox/6nsumcys.default/zotero/storage/4X6ZUQER/Maroko et al. - 2009 - The complexities of measuring access to parks and .pdf:application/pdf;Snapshot:/home/harry/.mozilla/firefox/6nsumcys.default/zotero/storage/ZBMIDKPQ/34.html:text/html}
