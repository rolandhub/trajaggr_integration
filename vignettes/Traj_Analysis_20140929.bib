@article{witten_neighborhood_2012,
	title = {Neighborhood Built Environment and Transport and Leisure Physical Activity: Findings Using Objective Exposure and Outcome Measures in New Zealand},
	volume = {120},
	issn = {0091-6765},
	shorttitle = {Neighborhood Built Environment and Transport and Leisure Physical Activity},
	url = {http://www.ncbi.nlm.nih.gov/pmc/articles/PMC3404665/},
	doi = {10.1289/ehp.1104584},
	abstract = {Background: Evidence of associations between neighborhood built environments and transport-related physical activity ({PA}) is accumulating, but few studies have investigated associations with leisure-time {PA}., Objective: We investigated associations of five objectively measured characteristics of the neighborhood built environment—destination access, street connectivity, dwelling density, land-use mix and streetscape quality—with residents’ self-reported {PA} (transport, leisure, and walking) and accelerometer-derived measures of {PA}., Methods: Using a multicity stratified cluster sampling design, we conducted a cross-sectional survey of 2,033 adults who lived in 48 New Zealand neighborhoods. Multilevel regression modeling, which was adjusted for individual-level (sociodemographic and neighborhood preference) and neighborhood-level (deprivation) confounders, was used to estimate associations of built environment with {PA}., Results: We found that 1-{SD} increases in destination access, street connectivity, and dwelling density were associated with any versus no self-reported transport, leisure, or walking {PA}, with increased odds ranging from 21\% [street connectivity with leisure {PA}, 95\% confidence interval ({CI}): 0\%, 47\%] to 44\% (destination accessibility with walking, 95\% {CI}: 17\%, 79\%). Among participants who self-reported some {PA}, a 1-{SD} increase in street connectivity was associated with a 13\% increase in leisure {PA} (95\% {CI}: 0, 28\%). {SD} increases in destination access, street connectivity, and dwelling density were each associated with 7\% increases in accelerometer counts., Conclusions: Associations of neighborhood destination access, street connectivity, and dwelling density with self-reported and objectively measured {PA} were moderately strong, indicating the potential to increase {PA} through changes in neighborhood characteristics.},
	number = {7},
	urldate = {2013-06-26},
	journal = {Environmental Health Perspectives},
	author = {Witten, Karen and Blakely, Tony and Bagheri, Nasser and Badland, Hannah and Ivory, Vivienne and Pearce, Jamie and Mavoa, Suzanne and Hinckson, Erica and Schofield, Grant},
	month = jul,
	year = {2012},
	pmid = {22456536},
	pmcid = {PMC3404665},
	keywords = {built environment, neighborhood, physical activity, walkability},
	pages = {971--977},
	file = {PubMed Central Full Text PDF:/home/harry/.mozilla/firefox/6nsumcys.default/zotero/storage/HX356B6S/Witten et al. - 2012 - Neighborhood Built Environment and Transport and L.pdf:application/pdf}
}

@article{weiss_reconsidering_2011,
	title = {Reconsidering Access: Park Facilities and Neighborhood Disamenities in New York City},
	volume = {88},
	issn = {1099-3460, 1468-2869},
	shorttitle = {Reconsidering Access},
	url = {http://link.springer.com/article/10.1007/s11524-011-9551-z},
	doi = {10.1007/s11524-011-9551-z},
	abstract = {With increasing concern about rising rates of obesity, public health researchers have begun to examine the availability of parks and other spaces for physical activity, particularly in cities, to assess whether access to parks reduces the risk of obesity. Much of the research in this field has shown that proximity to parks may support increased physical activity in urban environments; however, as yet, there has been limited consideration of environmental impediments or disamenities that might influence individuals’ perceptions or usage of public recreation opportunities. Prior research suggests that neighborhood disamenities, for instance crime, pedestrian safety, and noxious land uses, might dissuade people from using parks or recreational facilities and vary by neighborhood composition. Motivated by such research, this study estimates the relationship between neighborhood compositional characteristics and measures of park facilities, controlling for variation in neighborhood disamenities, using geographic information systems ({GIS}) data for New York City parks and employing both kernel density estimation and distance measures. The central finding is that attention to neighborhood disamenities can appreciably alter the relationship between neighborhood composition and spatial access to parks. Policy efforts to enhance the recreational opportunities in urban areas should expand beyond a focus on availability to consider also the hazards and disincentives that may influence park usage.},
	language = {en},
	number = {2},
	urldate = {2013-07-03},
	journal = {Journal of Urban Health},
	author = {Weiss, Christopher C. and Purciel, Marnie and Bader, Michael and Quinn, James W. and Lovasi, Gina and Neckerman, Kathryn M. and Rundle, Andrew G.},
	month = apr,
	year = {2011},
	keywords = {Built environment, Epidemiology, {GIS}, Health Informatics, Parks, Public Health/Gesundheitswesen, Spatial accessibility},
	pages = {297--310},
	file = {Full Text PDF:/home/harry/.mozilla/firefox/6nsumcys.default/zotero/storage/642HJ35C/Weiss et al. - 2011 - Reconsidering Access Park Facilities and Neighbor.pdf:application/pdf;Snapshot:/home/harry/.mozilla/firefox/6nsumcys.default/zotero/storage/URFK4V5M/s11524-011-9551-z.html:text/html}
}



@article{handy_measuring_1997,
	title = {Measuring accessibility: an exploration of issues and alternatives},
	volume = {29},
	shorttitle = {Measuring accessibility},
	url = {http://www.envplan.com/abstract.cgi?id=a291175},
	doi = {10.1068/a291175},
	abstract = {Accessibility is an important characteristic of metropolitan areas and is often reflected in transportation and land-use planning goals. But the concept of accessibility has rarely been translated into performance measures by which policies are evaluated, despite a substantial literature on the concept. This paper is an attempt to bridge the gap between the academic literature and the practical application of such measures and provide a framework for the development of accessibility measures. Issues that planners must address in developing an accessibility measure are outlined, and two case studies suggestive of the range of possible approaches are presented.},
	number = {7},
	urldate = {2014-07-28},
	journal = {Environment and Planning A},
