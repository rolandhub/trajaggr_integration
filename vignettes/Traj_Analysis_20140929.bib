	pmid = {18646974},
	keywords = {Adolescent, Adult, Aged, Aged, 80 and over, Age Factors, Auditory Threshold, Choice Behavior, Female, Humans, Male, Middle Aged, Noise, Railroads, Sleep Disorders},
