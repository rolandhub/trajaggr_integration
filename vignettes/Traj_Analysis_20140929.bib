	urldate = {2014-07-28},
	journal = {American Journal of Preventive Medicine},
	author = {Giles-Corti, Billie and Broomhall, Melissa H. and Knuiman, Matthew and Collins, Catherine and Douglas, Kate and Ng, Kevin and Lange, Andrea and Donovan, Robert J.},
	month = feb,
	year = {2005},
	pages = {169--176},
	file = {ScienceDirect Full Text PDF:/home/harry/.mozilla/firefox/6nsumcys.default/zotero/storage/7J5TJNSD/Giles-Corti et al. - 2005 - Increasing walking How important is distance to, .pdf:application/pdf;ScienceDirect Snapshot:/home/harry/.mozilla/firefox/6nsumcys.default/zotero/storage/MEGGSDH4/S0749379704002983.html:text/html}
}

